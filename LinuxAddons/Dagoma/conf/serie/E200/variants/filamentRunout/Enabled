Set FILAMENTCHANGE_FINALRETRACT -90
Enable FILAMENT_RUNOUT_SENSOR
Set FIL_RUNOUT_INVERTING false

# D66 is A11 : Button PIN
Set FILAMENT_RUNOUT_SCRIPT '"M600 P66"'

# D65 is A12 : Sensor PIN
Pin FILRUNOUT_PIN 65

Enable SUMMON_PRINT_PAUSE
Pin SUMMON_PRINT_PAUSE_PIN 66
Set SUMMON_PRINT_PAUSE_INVERTING false
# D66 is A11 : Button PIN
Set SUMMON_PRINT_PAUSE_SCRIPT '"M600 L0 P66"'
