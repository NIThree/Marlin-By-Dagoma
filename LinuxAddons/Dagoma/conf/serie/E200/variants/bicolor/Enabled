Set FILAMENT1CHANGE_EXTRACTION_SCRIPT '"M600 T0 I-1 U5 V195 X195 Y195"'
Set FILAMENT2CHANGE_EXTRACTION_SCRIPT '"M600 T1 I-1 U5 V195 X195 Y195"'

#Set EXTRUDE_MINTEMP 50
