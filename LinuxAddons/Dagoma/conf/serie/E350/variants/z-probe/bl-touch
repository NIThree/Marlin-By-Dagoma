Disable FIX_MOUNTED_PROBE

Set Z_MIN_ENDSTOP_INVERTING false

Set Y_MIN_POS -38
Set X_MAX_POS 370
Set Y_MAX_POS 395


Set LEFT_PROBE_BED_POSITION 10
Set RIGHT_PROBE_BED_POSITION 340
Set FRONT_PROBE_BED_POSITION 10
Set BACK_PROBE_BED_POSITION 340

Set X_PROBE_OFFSET_FROM_EXTRUDER 0
Set Y_PROBE_OFFSET_FROM_EXTRUDER 28
Set Z_PROBE_OFFSET_FROM_EXTRUDER 0

Set Z_RAISE_BEFORE_PROBING 10
Set Z_RAISE_AFTER_PROBING 10

Set Z_SAFE_HOMING_X_POINT '(175)'
Set Z_SAFE_HOMING_Y_POINT '(175)'


Enable NUM_SERVOS
Set NUM_SERVOS 1

Enable Z_ENDSTOP_SERVO_NR
Set Z_ENDSTOP_SERVO_NR 0

Enable SERVO_ENDSTOP_ANGLES
Set SERVO_ENDSTOP_ANGLES '{{0,0}, {0,0}, {10,90}}'

Pin SERVO0_PIN 19
Pin Z_MAX_PIN -1
